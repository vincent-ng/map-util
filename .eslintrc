{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": [2, "never"],
    "camelcase": 0,
    "indent": [2, "tab", { "SwitchCase": 1 }],
    "no-tabs": 0,
    "max-len": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-restricted-syntax": 0,
    "func-names": [2, "never"],
    "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
    }]
  }
}
